            <string>///Movement
motion(0,0,0,0,0,0)
if place_meeting(x,y+1,parSolid)
if !place_meeting(x,y+1,water_tile)
{ //Touches ground
vsp = -(irandom_range(5,7))
audio_play_sound(bubbly_bounce,1,false)
image_index = 0
image_speed = 0.25
}

if place_meeting(x,y+1,parSolid)
if place_meeting(x,y+1,water_tile)
{ //Touches ground
vsp = -3
audio_play_sound(bubbly_bounce,1,false)
image_index = 0
image_speed = 0.25
}

if place_meeting(x+1,y,parSolid) { //Hits wall on its right
facing = 180
}

if place_meeting(x-1,y,parSolid) { //Hits wall on its left
facing = 0
}

if facing = 0
if !place_meeting(x+1,y,parSolid)
{ hsp = 1 }
if facing = 180
if !place_meeting(x-1,y,parSolid)
{ hsp = -1 }

if place_meeting(x,y-1,water_tile)
{
grv_spd = 0.05
vsp -= 0.35
if i &lt; 3
{i += 0.5}
if i &gt;= 3
{
i = 0
instance_create(x-8,y+4,sweatpart)
audio_play_sound(splash,1,false)
audio_sound_pitch(splash,irandom_range(5,8)/10)
}
}

if place_meeting(x,y-1,water_tile)
if touchedwater = 0
{ 
touchedwater = 1
vsp = 1
}


if !place_meeting(x,y-1,water_tile)
{
grv_spd = 0.30
touchedwater = 0
}
</string>
