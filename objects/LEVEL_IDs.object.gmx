    /// B.B Angel IDs ///

    // i = world, j = level, k = id

    var tl = 0; // total levels
    var i = 0; // world
                    
    for (i = 0; i &lt; 9; i++ )
    {
        switch(i)
        {
            case 0: tl = 0; break;
            case 1: tl = 8; break;
            case 2: tl = 8; break;
            case 3: tl = 7; break;
            case 4: tl = 9; break;
            case 5: tl = 6; break;
            case 6: tl = 8; break;
            case 7: tl = 7; break;
            case 8: tl = 8; break;
            default: tl = 5;
        }
        for (j = 0; j &lt; tl+1; j++ )
        {
            global.bb_angel_id_1[i,j] = 0
            global.bb_angel_id_2[i,j] = 0
            global.bb_angel_id_3[i,j] = 0
        }
    }
