            <string>if keyboard_check(ord('0'))
if global.console_enabled = 0
{ 
global.musicvolume = 0
audio_sound_gain(global.BGMusic,global.musicvolume,0);
audio_sound_pitch(global.BGMusic,global.BGPitch)
}

if keyboard_check(ord('V'))
if keyboard_check(global.saved_rightkey)
{
if global.musicvolume &lt; 1
    {
    if keyboard_check(ord('X'))
        {
        global.musicvolume += 0.05
        }
    if !keyboard_check(ord('X'))
        {
        global.musicvolume += 0.005
        }
    var num = audio_get_listener_count();
    for( var i = 0; i &lt; num; i++;)
        {
        var info = audio_get_listener_info(i);
        audio_set_master_gain(info[? "index"], global.musicvolume);
        ds_map_destroy(info);
        }
    }
show_text = "Global Volume has been set to: "+string(global.musicvolume*100)+"%"
can_show_text = 1
alarm[4] = 180
}
if keyboard_check(ord('V'))
if keyboard_check(global.saved_leftkey)
{
if global.musicvolume &gt; 0
    {
    if keyboard_check(global.saved_dashkey)
        {
        global.musicvolume -= 0.05
        }
    if !keyboard_check(global.saved_dashkey)
        {
        global.musicvolume -= 0.005
        }
    var num = audio_get_listener_count();
    for( var i = 0; i &lt; num; i++;)
        {
        var info = audio_get_listener_info(i);
        audio_set_master_gain(info[? "index"], global.musicvolume);
        ds_map_destroy(info);
        }
    }
show_text = "Global Volume has been set to: "+string(global.musicvolume*100)+"%"
can_show_text = 1
alarm[4] = 180
}

if musicenabled = 1 {
audio_sound_gain(global.BGMusic,global.musicvolume,1)
audio_sound_pitch(global.BGMusic,global.BGPitch)
audio_sound_gain(global.SND,global.soundvolume,1)
}

if ((global.hour_time &gt; 17) || (global.hour_time &lt; 7)) // &gt;= 6:00pm and &lt; 7:00am
if global.ignore_time_phase == "false"
if instance_exists(mainchar)
if mainchar.powerup != "superpear"
if !instance_exists(bb_missile)
{ //It's Night
global.BGPitchMod = 0.85
