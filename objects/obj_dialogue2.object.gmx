draw_set_color(c_white);
t++;

//How many messages are in the array
message_end = array_length_1d(message)

if (message_end &gt; 0)
{
    //Variables
    var lineEnd = 35;
    var line = 0;
    var space = 1;
    var i = 1;
    var delay = 1;
    var mypitch = 1;
    
    //Text Position
    
    //Next Message
    if (keyboard_check_pressed(ord('S')))
    {
        //If we still have some more messages, go to the next one
        if (message_current &lt; message_end-1)
        {
            message_current++;
            cutoff = 0;
        }
        //If we dont, we're done
        else
        {
            done = true;
        }
    }
    
    //Typewriter
    if (cutoff &lt; string_length(message[message_current]))
    {
        if (timer &gt;= delay)
        {
            cutoff++;
            timer = 0;
            {
            audio_play_sound(counting,1,false)
            }
        }
        else timer++;
    }
    
    //Draw Text
    while(i &lt;= string_length(message[message_current]) &amp;&amp; i &lt;= cutoff)
    {
        //check for modifier
        if (string_char_at(message[message_current], i) == "\")
        {
            modifier = real(string_char_at(message[message_current], ++i));
            ++i;
        }
        //go to next line
        var length = 0;
        while (string_char_at(message[message_current], i) != " " &amp;&amp; i &lt;= string_length(message[message_current]))
        {
            i++;
            length++;
        }
        
        if (space+length &gt; lineEnd)
        {
            space = 0;
            line++;
        }
        i -= length;
        

        
        //Text
        switch(modifier)
        {
            case 0: //normal
            {
                text_draw(tX+(space*(charWidth)), tY+(13*line), string_char_at(message[message_current], i),c_white);
                mypitch = 1;
                break;
            }
            case 1: //shaky
            {
                text_draw(tX+(space*charWidth)+random_range(-1, 1), tY+(13*line)+random_range(-1, 1), string_char_at(message[message_current], i),c_white);
                mypitch = 0.7;
                break;
            }
            case 2: //Circle movement
            {
                var so = t + i;
                var shifty = sin(so*pi*freq/room_speed)*amplitude;
                var shiftx = cos(so*pi*freq/room_speed)*amplitude;
                text_draw(tX+(space*charWidth)+shiftx, tY+(13*line)+shifty, string_char_at(message[message_current], i),c_white);
                mypitch = 1;
                break;
            }
            case 3: //Sine movement (Titlescreen)
            {
                var so = t + i;
                var shifty = sin(so*pi*freq/room_speed)*amplitude;
                charWidth = font_get_size(myfont)+3;
                text_draw_offset(tX+((space*charWidth)+20), tY+(13*line)+shifty, string_char_at(message[message_current], i),c_white,4,4);
                mypitch = 1;
                break;
            }
        }
        space++;
        i++;
    }
    
}
